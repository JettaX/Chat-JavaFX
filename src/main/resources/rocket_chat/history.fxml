<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="rocket_chat.HistoryController"
      onMouseClicked="#mouseListener"
      prefHeight="400.0" prefWidth="600.0"
      stylesheets="@styles/common.css, @styles/history.css">
	<HBox fx:id="titleBox"/>
	<ScrollPane cache="true" cacheHint="SPEED" fitToWidth="true" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
		<VBox fx:id="messagesWrapper" VBox.vgrow="ALWAYS"/>
	</ScrollPane>
</VBox>
